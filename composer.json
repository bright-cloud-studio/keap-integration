{
    "name":"bright-cloud-studio/add-user-fields",
    "description":"Adds two custom user fields, image and bio, so they can be displayed on News posts and elsewhere",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "add", "user", "fields"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/add_user_fields/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/add_user_fields": "system/modules/add_user_fields"
            }
        }
    },
    "minimum-stability": "dev"
}
